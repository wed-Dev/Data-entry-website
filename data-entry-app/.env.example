# Environment Variables Template
# Copy this file to .env for local development
# Add these variables to Vercel project settings for production

# Vercel Postgres Database URL
# Get this from Vercel Dashboard > Storage > Postgres > .env.local
POSTGRES_URL="postgres://default:***@***-pooler.us-east-1.postgres.vercel-storage.vercel.app/verceldb?sslmode=require"
POSTGRES_PRISMA_URL="postgres://default:***@***-pooler.us-east-1.postgres.vercel-storage.vercel.app/verceldb?sslmode=require&pgbouncer=true&connect_timeout=15"
POSTGRES_URL_NON_POOLING="postgres://default:***@***-direct.us-east-1.postgres.vercel-storage.vercel.app/verceldb?sslmode=require"
POSTGRES_USER="default"
POSTGRES_HOST="***-pooler.us-east-1.postgres.vercel-storage.vercel.app"
POSTGRES_PASSWORD="***"
POSTGRES_DATABASE="verceldb"

# Optional: Custom port for local development
PORT=3000
